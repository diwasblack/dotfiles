# Custom tmux commands
[ -f ~/.config/zsh/tmuxf ] && source ~/.config/zsh/tmuxf

# Auto complete for tmuxinator commands
[ -f ~/.config/zsh/tmuxinator.zsh ] && source ~/.config/zsh/tmuxinator.zsh

# Configure FZF
[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
[ -f /usr/share/fzf/key-bindings.zsh ] && source /usr/share/fzf/key-bindings.zsh
[ -f /usr/share/fzf/completion.zsh ] && source /usr/share/fzf/completion.zsh

ZSH_COMMAND_NOT_FOUND='/usr/share/doc/pkgfile/command-not-found.zsh'
[[ -e $ZSH_COMMAND_NOT_FOUND ]] && source $ZSH_COMMAND_NOT_FOUND

export PATH="$PATH:$HOME/.local/bin"
